<%- include header %>
<% posts.forEach(function (post, index) { %>
  <p><h2><a href="/u/<%= post.author %>/<%= post.time.day %>/<%= post.title %>"><%= post.title %></a></h2></p>
  <p class="info">
    作者：<a href="/u/<%= post.author %>"><%= post.author %></a> | 
    日期：<%= post.time.minute %>
  </p>
  <p><%- post.content %></p>
<% }) %>
<%- include paging %>
<%- include footer %>